{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "main",
  "description": "Main Window Capability",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "core:app:allow-default-window-icon",
    "core:app:allow-set-app-theme",
    "core:window:allow-set-theme",
    "core:window:allow-theme",
    "store:default",
    "fs:default",
    "os:default",
    "os:allow-hostname"
  ]
}